{
    "version": 2,
    "builds": [
        {
            "src": "app_vercel.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app_vercel.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "SECRET_KEY": "@secret_key"
    }
}